{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/primary/env.d.ts",
    "src/renderer/src/primary/**/*",
    "src/renderer/src/primary/**/*.tsx",
    "src/preload/primary/**/*.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/primary/*"
      ]
    }
  }
}
