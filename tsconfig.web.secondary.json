{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/secondary/env.d.ts",
    "src/renderer/src/secondary/**/*",
    "src/renderer/src/secondary/**/*.tsx",
    "src/preload/secondary/**/*.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/secondary/*"
      ]
    }
  }
}
